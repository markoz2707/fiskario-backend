# Production Environment Configuration
NODE_ENV=production
EDEKLARACJE_TEST_ENV=false

# Database (production database)
DATABASE_URL="postgresql://prod_username:prod_password@prod-host:5432/fiskario_prod"

# JWT (production key - MUST BE CHANGED)
JWT_SECRET="CHANGE_THIS_TO_A_SECURE_PRODUCTION_SECRET_KEY"

# e-Deklaracje (production environment)
EDEKLARACJE_SOAP_TIMEOUT=60000
EDEKLARACJE_SOAP_RETRIES=5
EDEKLARACJE_TEST_WSDL_URL="https://test-e-deklaracje.mf.gov.pl/ws/e-Deklaracje.wsdl"
EDEKLARACJE_PRODUCTION_WSDL_URL="https://e-deklaracje.mf.gov.pl/ws/e-Deklaracje.wsdl"
EDEKLARACJE_CERT_PATH="/etc/ssl/certs/prod-e-deklaracje.crt"
EDEKLARACJE_KEY_PATH="/etc/ssl/private/prod-e-deklaracje.key"
EDEKLARACJE_CERT_PASSPHRASE=""

# Profil Zaufany API (production)
PROFIL_ZAUFANY_API_URL="https://pz.gov.pl/api"
PROFIL_ZAUFANY_CLIENT_ID=""
PROFIL_ZAUFANY_CLIENT_SECRET=""

# Monitoring (less frequent for production stability)
EDEKLARACJE_STATUS_CHECK_INTERVAL=30
EDEKLARACJE_AUTO_CHECK=true
EDEKLARACJE_MAX_RETRIES=5
EDEKLARACJE_RETRY_DELAY=10000

# Logging (production level)
LOG_LEVEL=warn

# Security (production keys - MUST BE CHANGED)
ENCRYPTION_KEY="CHANGE_THIS_TO_A_SECURE_32_CHAR_PRODUCTION_KEY!"